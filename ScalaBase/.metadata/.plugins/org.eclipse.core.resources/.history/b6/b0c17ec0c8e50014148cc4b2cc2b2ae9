package snippet;

public class Snippet {
	public static void main(String[] args) {
		package com{
		  package horstmann{
		    object A{
		      def hi=println("I am A")
		    }
		    package impatient{
		      object B extends App{
		        def hi=A.hi
		        hi
		      }
		    }
		  } 
		}
	}
}

